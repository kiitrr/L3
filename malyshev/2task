class ElementM:
    def __init__(self, name, symbol, number):
        self.name = name         
        self.symbol = symbol      
        self.number = number      

    def dump(self):
        print(f"Название: {self.name}, Символ: {self.symbol}, Атомный номер: {self.number}")


magnesium = ElementM("Магний", "Mg", 12)

magnesium.dump()
