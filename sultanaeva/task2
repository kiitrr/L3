class ElementS:
    def __init__(self, name, symbol, number):
        self.__name = name          
        self.__symbol = symbol      
        self.__number = number    

    @property
    def name(self):
        return self.__name

    @property
    def symbol(self):
        return self.__symbol

    @property
    def number(self):
        return self.__number

    def dump(self):
        print(f"Название: {self.name}, Символ: {self.symbol}, Атомный номер: {self.number}")


titan = ElementS("Титан", "Ti", 22)


titan.dump()
